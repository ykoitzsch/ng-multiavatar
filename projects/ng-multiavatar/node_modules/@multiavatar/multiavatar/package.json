{
  "name": "@multiavatar/multiavatar",
  "version": "1.0.3",
  "description": "Multicultural Avatar Generator",
  "author": "Gie Katon",
  "repository": {
    "type": "git",
    "url": "https://github.com/multiavatar/Multiavatar.git"
